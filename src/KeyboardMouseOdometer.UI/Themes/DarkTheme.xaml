<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Dark Theme Colors -->
    
    <!-- Main Window Colors -->
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#FF1E1E1E"/>
    <SolidColorBrush x:Key="WindowForegroundBrush" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="WindowBorderBrush" Color="#FF3F3F3F"/>
    
    <!-- Header Colors -->
    <SolidColorBrush x:Key="HeaderBackgroundBrush" Color="#FF2D2D30"/>
    <SolidColorBrush x:Key="HeaderForegroundBrush" Color="#FFF0F0F0"/>
    
    <!-- Content Area Colors -->
    <SolidColorBrush x:Key="ContentBackgroundBrush" Color="#FF252526"/>
    <SolidColorBrush x:Key="ContentForegroundBrush" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="ContentBorderBrush" Color="#FF3F3F3F"/>
    
    <!-- Tab Control Colors -->
    <SolidColorBrush x:Key="TabBackgroundBrush" Color="#FF2D2D30"/>
    <SolidColorBrush x:Key="TabForegroundBrush" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="TabSelectedBackgroundBrush" Color="#FF007ACC"/>
    <SolidColorBrush x:Key="TabSelectedForegroundBrush" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="TabBorderBrush" Color="#FF3F3F3F"/>
    
    <!-- Chart Colors -->
    <SolidColorBrush x:Key="ChartBackgroundBrush" Color="#FF1E1E1E"/>
    <SolidColorBrush x:Key="ChartGridLineBrush" Color="#FF3F3F3F"/>
    <SolidColorBrush x:Key="ChartAxisBrush" Color="#FF999999"/>
    <SolidColorBrush x:Key="ChartTextBrush" Color="#FFCCCCCC"/>
    
    <!-- Button Colors -->
    <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#FF3F3F46"/>
    <SolidColorBrush x:Key="ButtonForegroundBrush" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="ButtonBorderBrush" Color="#FF555555"/>
    <SolidColorBrush x:Key="ButtonHoverBackgroundBrush" Color="#FF4F4F56"/>
    <SolidColorBrush x:Key="ButtonPressedBackgroundBrush" Color="#FF2F2F36"/>
    
    <!-- TextBox Colors -->
    <SolidColorBrush x:Key="TextBoxBackgroundBrush" Color="#FF3F3F46"/>
    <SolidColorBrush x:Key="TextBoxForegroundBrush" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="TextBoxBorderBrush" Color="#FF555555"/>
    <SolidColorBrush x:Key="TextBoxDisabledBackgroundBrush" Color="#FF2D2D30"/>
    
    <!-- Statistics Colors -->
    <SolidColorBrush x:Key="StatValueBrush" Color="#FF40A0FF"/>
    <SolidColorBrush x:Key="StatLabelBrush" Color="#FF999999"/>
    
    <!-- Accent Colors -->
    <SolidColorBrush x:Key="AccentColorBrush" Color="#FF007ACC"/>
    <SolidColorBrush x:Key="AccentColorLightBrush" Color="#FF003D6B"/>  <!-- Dark blue background for light text -->
    <SolidColorBrush x:Key="AccentColorDarkBrush" Color="#FF005A9E"/>
    
    <!-- Status Colors (darker backgrounds for light text) -->
    <SolidColorBrush x:Key="SuccessBrush" Color="#FF1B5E20"/>  <!-- Dark green background -->
    <SolidColorBrush x:Key="WarningBrush" Color="#FFE65100"/>  <!-- Dark amber background -->
    <SolidColorBrush x:Key="ErrorBrush" Color="#FFB71C1C"/>   <!-- Dark red background -->
    <SolidColorBrush x:Key="InfoBrush" Color="#FF0D47A1"/>    <!-- Dark blue background -->
    
    <!-- Separator Colors -->
    <SolidColorBrush x:Key="SeparatorBrush" Color="#FF3F3F3F"/>
    
    <!-- ScrollBar Colors -->
    <SolidColorBrush x:Key="ScrollBarBackgroundBrush" Color="#FF2D2D30"/>
    <SolidColorBrush x:Key="ScrollBarThumbBrush" Color="#FF686868"/>
    <SolidColorBrush x:Key="ScrollBarThumbHoverBrush" Color="#FF9E9E9E"/>
    
    <!-- Toolbar Colors (for floating toolbar) -->
    <SolidColorBrush x:Key="ToolbarBackgroundBrush" Color="#CC1E1E1E"/>
    <SolidColorBrush x:Key="ToolbarForegroundBrush" Color="#FFF0F0F0"/>
    <SolidColorBrush x:Key="ToolbarBorderBrush" Color="#FF3F3F3F"/>
    
    <!-- Heatmap Colors -->
    <SolidColorBrush x:Key="HeatmapBackgroundBrush" Color="#FF1E1E1E"/>
    <SolidColorBrush x:Key="HeatmapKeyBackgroundBrush" Color="#FF2D2D30"/>
    <SolidColorBrush x:Key="HeatmapKeyBorderBrush" Color="#FF3F3F3F"/>
    <SolidColorBrush x:Key="HeatmapKeyTextBrush" Color="#FFCCCCCC"/>
    
    <!-- Shadow Effects -->
    <Color x:Key="ShadowColor">#80000000</Color>
    
</ResourceDictionary>